{
  "name": "livecells",
  "version": "0.1.0",
  "description": "Add live programming to Jupyter cells",
  "author": "",
  "main": "lib/lab/index.js",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "build_parser": "node ./node_modules/jison/lib/cli.js --outfile src/parsers/python/python3.js src/parsers/python/python3.jison",
    "watch": "tsc -w",
    "test": "mocha -r ts-node/register  src/**/*.test.ts"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true
  },
  "dependencies": {
    "@jupyterlab/application": "^0.16.3",
    "@jupyterlab/apputils": "^0.16.4",
    "@jupyterlab/coreutils": "^2.0.0-0",
    "@jupyterlab/docmanager": "^0.16.3",
    "@jupyterlab/fileeditor": "^0.16.3",
    "@jupyterlab/notebook": "^0.16.3",
    "@phosphor/widgets": "^1.6.0",
    "@types/jquery": "^3.3.4",
    "jquery": "^3.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/codemirror": "0.0.58",
    "@types/mocha": "^5.2.4",
    "@types/node": "^8.0.51",
    "chai": "^4.1.2",
    "jison": "^0.4.18",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.1",
    "ts-node": "^7.0.0",
    "typescript": "~2.4.0"
  }
}
